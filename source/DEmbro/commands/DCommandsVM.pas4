unit DCommandsVM;

interface

define(`cmdhdr', `(Machine: TForthMachine; Command: PForthCommand)')

uses
  DForthMachine;

  procedure vm_step cmdhdr;  

procedure LoadCommands(Machine: TForthMachine);

implementation

       procedure vm_step cmdhdr;
       begin
         Writeln('vm-step');
         Machine.Step;
       end;

procedure LoadCommands(Machine: TForthMachine);
begin
  Machine.AddCommand('vm-step', vm_step);  
end;

end.
