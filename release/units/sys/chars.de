// Важные команды по работе с символами

32 constant SP // space
13 constant CR ( h0D) // aka \r
10 constant LF ( h0A) // aka \n
h0085 constant NEL // unicode next line
h2028 constant LS // unicode line separator
h2029 constant PS // paragraph separator

: CR->CR+LF ;
: LF->CR+LF ;
