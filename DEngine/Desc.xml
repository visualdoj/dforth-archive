        <short>Shirina zadannoi texturi.</short>
      </element>
      <element name="TGraph.TexHeight">
        <short>Visota zadannoi texturi.</short>
      </element>
      <element name="TGraph.GetTexFileName">
        <short>Imya faila texturi.</short>
      </element>
      <element name="TGraph.SetPointSize">
        <short>Ustanavlivaet razmer tochki.</short>
      </element>
      <element name="TGraph.SetLineWidth">
        <short>Ustanavlivaet shirinu linii.</short>
      </element>
      <element name="TGraph.SetTexture">
        <short>Ustanavlivaet tekushuu texturu.</short>
      </element>
      <element name="TGraph.SetColor">
        <short>Ustanavlivaet tekushii cvet.</short>
      </element>
      <element name="TGraph.Color4f">
        <short>Ustanavlivaet tekushii cvet.</short>
      </element>
