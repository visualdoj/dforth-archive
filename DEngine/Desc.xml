        <short>Risuet liniu.</short>
      </element>
      <element name="TGraph.DrawRect">
        <short>Risuet Rect. Fill - nujno li zakrashivat' vnutrennost'</short>
      </element>
      <element name="TGraph.DrawCircle">
        <short>Risuet okrujnost'.</short>
      </element>
      <element name="TGraph.DrawPolygon">
        <short>Risuet mnogougol'nik.</short>
      </element>
      <element name="TGraph.DrawTexture">
        <short>Risuet pryamougol'nik s nalojennoi texturoi.</short>
      </element>
      <element name="TGraph.DrawTiles">
