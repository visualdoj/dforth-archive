    <module name="DIntersection">
      <element name="Contain">
        <short>Proveryaet soderjisya li B vnutri A.</short>
      </element>
      <element name="Intersect">
        <short>Proveryaet stalkivautsya li dve figuri.</short>
      </element>
    </module>

    <module name="DCamera2D">
      <element name="TCamera2D">
        <short>Realizuet scrolling v dvumernoi igre.</short>
      </element>
      <element name="TCamera2D.Bind">
        <short>Ne ispol'zuetsya.</short>
      </element>
      <element name="TCamera2D.Focus">
        <short>Ustanavlivaet matricu rendera v sootvetstvii s polojeniem cameri.</short>
      </element>
      <element name="TCamera2D.Unfocus">
        <short>Vozvrashaet sostoyanie cameri, kotoroe bilo do vizova Focus</short>
      </element>
      <element name="TCamera2D.Update">
        <short>Obnovlyaet sostoyanie cameri, v kachestve parametra peredaetsya polojenie ob'ekta, kotorii doljen bit' viden na ekrane (naprimer, GG).</short>
      </element>
      <element name="TCamera2D.Center">
        <short>Oblast' na ekrane, v kotoroi ob'ekt mojet peremesh'at'sya i pri etom camera budet stoyat' na meste</short>
      </element>
    </module>

