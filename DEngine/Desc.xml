  <package name="DEngine">    
    <module name="DUtils.pas">
      <element name="GetTimer">
        <short>Возвращает текущее время в мс.</short>
      </element>
      <element name="PCrossChar">
        <short>Кросскомпиляторная строка.</short>
      </element>
    </module>
    <module name="DGraph">
      <element name="TBlendMode">
        <short>Operaciya, kotoraya budet vipolnyatsya pri zapisi pixelya na ekran</short>
      </element>
      <element name="TBlendMode.bmNo">
        <short>Prosto zapicat'</short>
      </element>
      <element name="TBlendMode.bmAdd">
        <short>Slojit' vhodyashii cvet s cvetom na ekrane</short>
      </element>
      <element name="TBlendMode.bmMul">
        <short>Umnojit' vhodyashii cvet s cvetom na ekrane</short>
      </element>
      <element name="TBlendMode.bmAlpha">
        <short>Ispol'zovat' Alphu vhodyashego cveta, 0 - polnost'u nevidimii pixel, 1 - polnost'u vidimii.</short>
      </element>
      <element name="TBlendMode.bmDiffuse">
        <short>Ispol'zovat' vhodyashii cvet v kachestve alphi. Chem temnee - tem prozrachnei.</short>
      </element>
      <element name="TProjMode">
        <short>Poka chto ne ispol'zuetsya.</short>
      </element>
      <element name="TTexture">
        <short>Identificator texturi v dvijke.</short>
      </element>
      <element name="TTextureInfo">
        <short>Informaciya o texture.</short>
      </element>
      <element name="TTgaHeader">
        <short>Zagolovok tga faila.</short>
      </element>
      <element name="TGraph">
        <short>Ob'ekt cherez kotorii proishodit ves' render.</short>
      </element>
      <element name="TGraph.BeginRender2D">
        <short>Vkluchaet 2d render.</short>
      </element>
